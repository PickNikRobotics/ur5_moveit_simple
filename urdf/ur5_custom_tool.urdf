<?xml version="1.0" ?>
<robot name="kuka_custom_tool" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="tool0"/>

  <link name="tool_custom"/>

  <joint name="tool0-tool_custom" type="fixed">
    <parent link="tool0"/>
    <child link="tool_custom"/>
    <origin rpy="1.5707 0 3.14159" xyz="0.0 0.0 0.0"/>
  </joint>

  <!-- Robotiq -->
  <xacro:include filename="$(find robotiq_2f_model)/model/robotiq_2f_85.urdf.xacro" />

  <xacro:robotiq_2f_85  name="robotiq_2f_85" parent="tool0">
    <origin xyz="0.0 0.0 0.01" rpy="0.0 0.0 0.0" />
  </xacro:robotiq_2f_85>

</robot>
